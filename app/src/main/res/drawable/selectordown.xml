<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 填充的颜色 -->
    <solid android:color="#65cce7" />

    <!-- 设置按钮的四个角为弧形 -->
    <corners
        android:radius="10dp" />

    <!-- padding：Button里面的文字与Button边界的间隔 -->
    <padding
        android:bottom="4dp"
        android:left="8dp"
        android:right="8dp"
        android:top="4dp" />

</shape>